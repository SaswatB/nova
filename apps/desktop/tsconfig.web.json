{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/renderer/src/env.d.ts",
    "src/renderer/src/**/*",
    "src/renderer/src/**/*.tsx",
    "src/preload/*.d.ts",
    "styled-system"
  ],
  "compilerOptions": {
    "composite": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "noImplicitAny": null,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "paths": {
      "@renderer/*": ["src/renderer/src/*"]
    }
  }
}

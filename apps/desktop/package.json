{
  "name": "desktop",
  "version": "1.0.0",
  "description": "An Electron application with React and TypeScript",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "meta": "doppler run -- jiti src/renderer/src/lib/prototype/meta-nova-dev.ts",
    "dev": "concurrently --kill-others \"VITE_ENABLE_STT=true yarn dev:vite\" \"yarn dev:stt\"",
    "dev:vite": "doppler run -- electron-vite dev",
    "dev:stt": "python3 /Users/saswat/Documents/clones/RealtimeSTT/example_webserver/server.py",
    "dev:meta": "nodemon --watch src --ext ts,tsx --exec 'yarn meta'",
    "prepare": "panda codegen",
    "build": "npm run typecheck && electron-vite build",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.20.9",
    "@dagrejs/dagre": "^1.1.2",
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^3.0.0",
    "@fontsource-variable/inter": "^5.0.17",
    "@google/generative-ai": "^0.11.3",
    "@hookform/resolvers": "^3.4.2",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/themes": "^3.0.5",
    "@xyflow/react": "^12.0.0-next.18",
    "axios": "^1.6.8",
    "electron-screenshots": "^0.5.26",
    "electron-updater": "^6.1.7",
    "events": "^3.3.0",
    "groq-sdk": "^0.3.3",
    "idb-keyval": "^6.2.1",
    "ignore": "^5.3.1",
    "immer": "^10.0.4",
    "llama-tokenizer-js": "^1.2.1",
    "lodash": "^4.17.21",
    "nanoid": "^5.0.7",
    "openai": "^4.47.1",
    "react-async-hook": "^4.0.0",
    "react-hook-form": "^7.51.5",
    "react-toastify": "^10.0.5",
    "split-pane-react": "patch:split-pane-react@npm%3A0.1.3#~/.yarn/patches/split-pane-react-npm-0.1.3-1cc8da6424.patch",
    "ts-pattern": "^5.1.1",
    "typescript": "^5.4.5",
    "zod": "^3.22.5",
    "zod-to-json-schema": "^3.23.0"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^1.0.1",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@pandacss/dev": "^0.37.2",
    "@repo/eslint-config": "workspace:*",
    "@types/events": "^3",
    "@types/lodash": "^4.17.4",
    "@types/node": "^18.19.9",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/wicg-file-system-access": "^2023.10.5",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "electron": "28.3.0",
    "electron-builder": "^24.9.1",
    "electron-vite": "^2.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "jiti": "^1.21.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "unplugin-fonts": "^1.1.1",
    "vite": "^5.0.12"
  }
}
